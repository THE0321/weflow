<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.messanger.mapper.AIMapper">

    <resultMap id="AISchemaMap" type="com.project.messanger.dto.AISchemaDto">
        <id property="schemaIdx" column="schema_idx"/>
        <result property="feature" column="feature"/>
        <result property="isUse" column="is_use"/>
        <result property="isRequired" column="is_required"/>
        <result property="columnKey" column="column_key"/>
        <result property="name" column="name"/>
        <result property="description" column="description"/>
    </resultMap>

    <select id="getFeatureList" resultMap="string">
        SELECT DISTINCT feature
        FROM ai_schema
        WHERE is_use = 'Y'
    </select>

    <select id="getColumnList" parameterType="" resultMap="">
        SELECT *
        FROM ai_schema
        WHERE
    </select>
</mapper>